{
  "plugins": [
    [
      "inline-json-import",
      {}
    ],
    "@babel/plugin-proposal-class-properties"
  ],
  "presets": [
    "@babel/preset-react",
    [
      "@babel/preset-typescript",
      {
        "allExtensions": true,
        "isTSX": true
      }
    ],
    "@babel/preset-env"
  ],
  "env": {
    "production": {
      "presets": [
        "minify"
      ],
      "sourceMaps": true
    }
  }
}