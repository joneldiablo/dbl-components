{
  "name": "dbl-components",
  "version": "1.2.10",
  "description": "Framework based on bootstrap 5",
  "main": "lib/js/index.js",
  "scripts": {
    "start": "yarn storybook",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -p 6006 --no-dll",
    "build-storybook": "build-storybook --no-dll -o docs",
    "babel": "babel src/js -d lib/js --minified --source-maps",
    "clear:js": "rm -rf lib/js/* && mkdir -p lib/js",
    "clear:css": "rm -rf lib/css/* && mkdir -p lib/css",
    "clear:scss": "rm -rf lib/scss/* && mkdir -p lib/scss",
    "clear": "yarn clear:js && yarn clear:css && yarn clear:scss",
    "css": "node-sass --importer node_modules/node-sass-import src/scss/style.scss lib/css/style.css && yarn postcss",
    "postcss": "postcss lib/css/style.css --use autoprefixer -d lib/css --no-map",
    "scss": "cp -r src/scss lib",
    "build": "yarn clear && yarn scss && yarn babel && yarn build-storybook"
  },
  "keywords": [
    "bs",
    "bootstrap",
    "react",
    "storybook",
    "bootstrap 5",
    "bs5"
  ],
  "author": "joneldiablo",
  "license": "ISC",
  "peerDependencies": {
    "@popperjs/core": "^2.7.0",
    "bootstrap": "^5.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.12.1",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-controls": "^6.1.21",
    "@storybook/addon-docs": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/core-server": "^6.2.1",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.1.21",
    "autoprefixer": "^10.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-inline-json-import": "^0.3.2",
    "babel-plugin-polyfill-corejs3": "^0.2.0",
    "node-sass": "^5.0.0",
    "node-sass-import": "^2.0.1",
    "postcss": "^8.1.6",
    "postcss-cli": "^8.2.0",
    "react-is": "^17.0.1",
    "sass-loader": "^10.0.5"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-react": "^3.0.2",
    "@splidejs/react-splide": "^0.4.4",
    "css-element-queries": "^1.2.3",
    "details-polyfill": "^1.2.0",
    "flat": "^5.0.2",
    "html-react-parser": "^1.2.4",
    "jexcel": "^4.4.1",
    "konva": "^7.2.0",
    "moment": "^2.29.1",
    "pluralize-es": "^1.0.0",
    "react-icomoon": "^2.1.5",
    "react-konva": "^17.0.0-0",
    "react-youtube": "^7.13.1",
    "resize-observer": "^1.0.2",
    "url-join": "^4.0.1"
  }
}